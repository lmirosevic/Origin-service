{
	"PERSISTENCE": {
		"type": "InMemory",
		"options": { 
			"url": "redis://localhost:27017/",
			"collectionNamespace": "origin"
		}
	},
	"SERVICE_INTERFACE": {
		"type": "Resque",
		"options": {
			"redis": "redis://localhost:6379/4",
			"queue": "origin.serviceInterface",
			"maxReconnectionTimeout": 1000
		}
	},
	"FRONTEND" : {
		"type": "ZeroMQ",
		"options": {
			"port": 56301,
			"subscriptionKeepaliveTimeout": 80
		}
	},
	"LOG_SERVICE_UPDATES": true
}
