{
	"PERSISTENCE": {
		"type": "Redis",
		"options": { 
			"url": "redis://localhost:6379/5",
			"collectionNamespace": "origin",
			"maxReconnectionTimeout": 1000
		}
	},
	"SERVICE_INTERFACE": {
		"type": "Resque",
		"options": {
			"redis": "redis://localhost:6379/4",
			"queue": "origin.serviceInterface",
			"maxReconnectionTimeout": 1000,
			"logUpdates": true
		}
	},
	"FRONTEND" : {
		"type": "ZeroMQ",
		"options": {
			"port": 56301,
			"subscriptionKeepaliveTimeout": 80,
			"logSignificantUpdates": true
		}
	}
}
